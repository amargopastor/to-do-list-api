{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": ["airbnb-base"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"camelcase": "off",
		"import/extensions": "off",
		"import/no-unresolved": "off",
		"import/prefer-default-export": "off",
		"import/no-extraneous-dependencies": "off"
	}
}
